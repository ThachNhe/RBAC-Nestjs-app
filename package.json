{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -b swc",
    "start:dev": "nest start -b swc --watch",
    "start:debug": "nest start -b swc --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand --forceExit --detectOpenHandles",
    "mikro-orm": "mikro-orm",
    "db:up": "mikro-orm migration:up",
    "db:migrate": "mikro-orm schema:drop && mikro-orm migration:create",
    "db:fresh": "mikro-orm migration:fresh",
    "db:seed": "mikro-orm migration:fresh --seed",
    "db:test": "NODE_ENV=test mikro-orm migration:fresh --seed"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.670.0",
    "@aws-sdk/s3-presigned-post": "^3.670.0",
    "@aws-sdk/s3-request-presigner": "^3.670.0",
    "@blazity/nest-file-fastify": "^1.0.0",
    "@fastify/cookie": "^9.4.0",
    "@fastify/multipart": "6.0.0",
    "@fastify/static": "^7.0.4",
    "@litehex/node-vault": "^1.0.0",
    "@mikro-orm/cli": "^6.3.13",
    "@mikro-orm/core": "^6.3.13",
    "@mikro-orm/migrations": "^6.3.13",
    "@mikro-orm/nestjs": "^6.0.2",
    "@mikro-orm/postgresql": "^6.3.13",
    "@mikro-orm/seeder": "^6.3.13",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.1.2",
    "@nestjs/bullmq": "^10.2.1",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.4.4",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.2.3",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.16.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "generate-password": "^1.7.1",
    "handlebars": "^4.7.8",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "mikro-orm": "^6.3.13",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "nestjs-pino": "^4.1.0",
    "nodemailer": "^6.9.15",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^11.2.2",
    "pino-syslog": "^3.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "snowyflake": "^2.0.0",
    "uuidv7": "^1.0.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.4",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.26",
    "@swc/jest": "^0.2.36",
    "@types/bcrypt": "^5.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.13",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.9.2",
    "@types/nodemailer": "^6.4.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "@swc/jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
